<view class="content"><uni-swiper-dot vue-id="1" info="{{info}}" current="{{current}}" field="content" mode="{{mode}}" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="swiper-box" autoplay="true" data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><block wx:for="{{info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image class="image" src="{{item}}" mode="aspectFill"></image></view></swiper-item></block></swiper></uni-swiper-dot><view class="view-title">{{confmodel.name}}</view><block wx:if="{{confmodel.meetdate!=null}}"><view class="view-subtitle">{{'会议时间：'+confmodel.meetdate+''}}</view></block><block wx:if="{{confmodel.meetaddress!=null}}"><view class="view-subtitle">{{'会议地点：'+confmodel.meetaddress+''}}</view></block><block wx:if="{{confmodel.unsignedcontentq!=null}}"><view class="view-subtitle">{{''+confmodel.unsignedcontentq+''}}</view></block><view class="view-signin"><button open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['getPhoneNumber',['$event']]]]]}}" bindgetphonenumber="__e">点击签到</button></view><block wx:if="{{hassignin=='Y'}}"><view class="view-subtitle">{{'您是第'+signordernum+'个签到的参会者。'}}</view></block><view class="view-line"></view><view class="view-body"><ss-navbar vue-id="2" navArr="{{navArr3}}" tabCurrentIndex="{{currentIndex3}}" data-event-opts="{{[['^navbarTap',[['navbarTapHandler3']]]]}}" bind:navbarTap="__e" bind:__l="__l"></ss-navbar><view class="view-content"><block wx:if="{{confmodel.requiredsiginin=='N'||confmodel.requiredsiginin=='Y'&&hassignin=='Y'}}"><rich-text nodes="{{navArr3str}}"></rich-text></block><block wx:if="{{confmodel.requiredsiginin=='Y'&&hassignin=='N'}}"><rich-text>请您先签到！</rich-text></block></view></view></view>